matrices = []
location = []
for i in range(5):
    x = list(map(int,input().split()))
    matrices.append(x)
y = len(matrices)
for i in range(y):
    for j in range(len(matrices[i])):
        if matrices[i][j] == 1:
            location.append(i+1)
            location.append(j+1)
            break
X = abs(location[0]-3)
Y = abs(location[1]-3)
print(X+Y)

        
